<h2>IronHack Cinema</h2>
<h3>Click on the movie to check the showtimes!</h3>

<div class="gallery">
  {{#each movies}}
  <div class="card">
    <img src="{{this.image}}" alt="" class="card__img">
    <h4 class="card__title">{{this.title}}</h4>
    <div>
      <a class="button" href="/movies/{{this._id}}">See more</a>
    </div>
  </div>
  {{/each}}
</div>
